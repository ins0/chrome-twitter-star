{
  "name": "Twitter Star",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Return of the Twitter Tweet Star",
  "homepage_url": "https://github.com/ins0/chrome-twitter-star",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["src/twitter-star.css"]
    }
  ]
}
